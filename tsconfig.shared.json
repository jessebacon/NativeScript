{
    "compilerOptions": {
        "noEmitOnError": false,
        "noEmitHelpers": true,
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "noImplicitUseStrict": true,
        "removeComments": true,
        "experimentalDecorators": true,
        "diagnostics": true,
        "inlineSourceMap": true,
        "jsx": "react",
        "reactNamespace": "UIBuilder",
        "lib": [
            "es6", "dom"
        ],
        "types": []
    }
}
